(func trundle [a:int b:string] [n:int err:error]
  (return 78 nil))

(assert (== 78 (trundle a:3 b:"hi")))

(expect-error `Error calling 'trundle': cannot pass 'int64' to argument 'b' of type 'string'` (trundle a:3 b:5))

